<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for Encumberance.order -->
        <record id="seq_encumb_order" model="ir.sequence">
            <field name="name">Encumbrance Order</field>
            <field name="code">encumb.order</field>
            <field name="prefix">ENC/%(range_year)s/</field>
           <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">3</field>
        </record>

    </data>
</odoo>
